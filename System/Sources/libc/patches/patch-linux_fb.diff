diff -Naur libc-0.1.2.orig/include/kernel/linux/linux/Makefile libc-0.1.2/include/kernel/linux/linux/Makefile
--- libc-0.1.2.orig/include/kernel/linux/linux/Makefile	1970-01-01 01:00:00.000000000 +0100
+++ libc-0.1.2/include/kernel/linux/linux/Makefile	2015-05-23 16:33:51.000000000 +0200
@@ -0,0 +1,33 @@
+OBJDIR	=
+PREFIX	= /usr/local
+DESTDIR	=
+RM	= rm -f
+LN	= ln -f
+MKDIR	= mkdir -m 0755 -p
+INSTALL	= install
+INCLUDEDIR= $(PREFIX)/include
+
+
+all:
+
+clean:
+
+distclean: clean
+
+install:
+	$(MKDIR) $(DESTDIR)$(INCLUDEDIR)/linux
+	$(INSTALL) -m 0644 fb.h $(DESTDIR)$(INCLUDEDIR)/linux/fb.h
+	$(MKDIR) $(DESTDIR)$(INCLUDEDIR)/linux
+	$(INSTALL) -m 0644 kd.h $(DESTDIR)$(INCLUDEDIR)/linux/kd.h
+	$(MKDIR) $(DESTDIR)$(INCLUDEDIR)/linux
+	$(INSTALL) -m 0644 keyboard.h $(DESTDIR)$(INCLUDEDIR)/linux/keyboard.h
+	$(MKDIR) $(DESTDIR)$(INCLUDEDIR)/linux
+	$(INSTALL) -m 0644 vt.h $(DESTDIR)$(INCLUDEDIR)/linux/vt.h
+
+uninstall:
+	$(RM) -- $(DESTDIR)$(INCLUDEDIR)/linux/fb.h
+	$(RM) -- $(DESTDIR)$(INCLUDEDIR)/linux/kd.h
+	$(RM) -- $(DESTDIR)$(INCLUDEDIR)/linux/keyboard.h
+	$(RM) -- $(DESTDIR)$(INCLUDEDIR)/linux/vt.h
+
+.PHONY: all clean distclean install uninstall
