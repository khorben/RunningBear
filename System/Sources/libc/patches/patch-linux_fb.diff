+/* The major part of this file was taken from the Linux kernel */
+
+
+
+#ifndef LIBC_KERNEL_LINUX_LINUX_FB_H
+# define LIBC_KERNEL_LINUX_LINUX_FB_H
+
+
+/* types */
+/* FIXME should not be here */
+typedef char * caddr_t;
+
+# if defined(__linux__)
+struct fb_bitfield
+{
+	uint32_t offset;
+	uint32_t length;
+	uint32_t msb_right;
+};
+
+struct fb_cmap
+{
+	uint32_t start;
+	uint32_t len;
+	uint16_t * red;
+	uint16_t * green;
+	uint16_t * blue;
+	uint16_t * transp;
+};
+
+struct fb_fix_screeninfo
+{
+	char id[16];
+	unsigned long smem_start;
+	uint32_t smem_len;
+	uint32_t type;
+	uint32_t type_aux;
+	uint32_t visual;
+	uint16_t xpanstep;
+	uint16_t ypanstep;
+	uint16_t ywrapstep;
+	uint32_t line_length;
+	unsigned long mmio_start;
+	uint32_t mmio_len;
+	uint32_t accel;
+	uint16_t _padding[3];
+};
+
+struct fb_var_screeninfo
+{
+	uint32_t xres;
+	uint32_t yres;
+	uint32_t xres_virtual;
+	uint32_t yres_virtual;
+	uint32_t xoffset;
+	uint32_t yoffset;
+	uint32_t bits_per_pixel;
+	uint32_t grayscale;
+	struct fb_bitfield red;
+	struct fb_bitfield green;
+	struct fb_bitfield blue;
+	struct fb_bitfield transp;
+	uint32_t nonstd;
+	uint32_t activate;
+	uint32_t height;
+	uint32_t width;
+	uint32_t accel_flags;
+	uint32_t pixclock;
+	uint32_t left_margin;
+	uint32_t right_margin;
+	uint32_t upper_margin;
+	uint32_t lower_margin;
+	uint32_t hsync_len;
+	uint32_t vsync_len;
+	uint32_t sync;
+	uint32_t vmode;
+	uint32_t rotate;
+	uint32_t _padding[5];
+};
+# elif defined(__NetBSD__)
+struct fb_cmap
+{
+	uint32_t start;
+	uint32_t len;
+	unsigned char * red;
+	unsigned char * green;
+	unsigned char * blue;
+	/* XXX not in original, hoping it'll be ignored */
+	unsigned char * transp;
+};
+
+struct fb_fix_screeninfo
+{
+	uint32_t type;
+};
+
+struct fb_var_screeninfo
+{
+	uint32_t height;
+	uint32_t width;
+	uint32_t bits_per_pixel;
+	uint32_t cmsize;
+};
+# else
+#  warning Unsupported platform
+# endif
+
+
+/* constants */
+# if defined(__linux__)
+#  define FBIOGET_VSCREENINFO		0x4600
+#  define FBIOPUT_VSCREENINFO		0x4601
+#  define FBIOGET_FSCREENINFO		0x4602
+#  define FBIOGETCMAP			0x4604
+#  define FBIOPUTCMAP			0x4605
+
+#  define FB_SYNC_HOR_HIGH_ACT		1
+#  define FB_SYNC_VERT_HIGH_ACT		2
+#  define FB_SYNC_EXT			4
+#  define FB_SYNC_COMP_HIGH_ACT		8
+#  define FB_SYNC_BROADCAST		16
+
+#  define FB_TYPE_PACKED_PIXELS		0
+#  define FB_TYPE_PLANES		1
+#  define FB_TYPE_INTERLEAVED_PLANES	2
+#  define FB_TYPE_TEXT			3
+#  define FB_TYPE_VGA_PLANES		4
+
+#  define FB_VISUAL_MONO01		0
+#  define FB_VISUAL_MONO10		1
+#  define FB_VISUAL_TRUECOLOR		2
+#  define FB_VISUAL_PSEUDOCOLOR		3
+#  define FB_VISUAL_DIRECTCOLOR		4
+#  define FB_VISUAL_STATIC_PSEUDOCOLOR	5
+
+#  define FB_VMODE_NINTERLACED		0
+#  define FB_VMODE_INTERLACED		1
+#  define FB_VMODE_DOUBLE		2
+#  define FB_VMODE_ODD_FLD_FIRST	4
+#  define FB_VMODE_MASK			255
+# elif defined(__NetBSD__)
+#  include <sys/ioctl.h>
+#  define FBIOGET_FSCREENINFO		_IOR('W', 64, struct fb_fix_screeninfo)
+/* XXX doesn't exist */
+#  define FBIOPUT_FSCREENINFO		_IOW('W', 64, struct fb_fix_screeninfo)
+#  define FBIOGET_VSCREENINFO		_IOR('W', 65, struct fb_var_screeninfo)
+/* XXX doesn't exist */
+#  define FBIOPUT_VSCREENINFO		_IOW('W', 65, struct fb_var_screeninfo)
+#  define FBIOGETCMAP			_IOW('W', 66, struct fb_cmap)
+#  define FBIOPUTCMAP			_IOW('W', 67, struct fb_cmap)
+
+/* XXX necessary to mmap() */
+#  define WSDISPLAYIO_SMODE		_IOW('W', 76, uint32_t)
+#  define WSDISPLAYIO_MODE_DUMBFB	2
+# else
+#  warning Unsupported platform
+# endif
+
+#endif /* !LIBC_KERNEL_LINUX_LINUX_FB_H */
